#pragma once

#include <iostream>


//
template <class T> 
void vectorInfo(std::vector<T> v) {
    std::cout  << "size( ): " << v.size( ) 
        << "; capacity(): " << v.capacity() 
        << "; max_size(): " << v.max_size() << std::endl;
}

//
template <class T> 
void vectorPrint(std::vector<T> v) {
    for(int i =0;i<v.size(); i ++) {
        std::cout << "v.at(" << i << ") " << v.at(i) << std::endl;
    } 
}

